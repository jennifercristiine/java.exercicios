public class Exer5 
{
    public static void main(String[] args) 
    {
        Carro meuCarro = new Carro("Peugeot", "208", 2013, "SDE5L18", 45);

        meuCarro.acelerar();
        meuCarro.acelerar();
        
        meuCarro.exibirInfo();
        
        meuCarro.frear();
        
        meuCarro.exibirInfo();
    }
}
